{
    "entryFile": "src/index.ts",
    "controllerPathGlobs": [
        "src/controllers/*Controller.ts"
    ],
    "spec": {
        "outputDirectory": "dist",
        "specVersion": 3,
        "securityDefinitions": {
            "jwt": {
                "type": "apiKey",
                "name": "Authorization",
                "in": "header"
            }
        }
    },
    "routes": {
        "routesDir": "src/routes"
    }
}